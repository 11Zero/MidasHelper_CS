                    </Grid>
                </GroupBox>
                <Button Content="生成MCT" Height="23" HorizontalAlignment="Left" Margin="558,447,0,0" Name="btn_export_mct" VerticalAlignment="Top" Width="75" Click="btn_export_mct_Click" />
