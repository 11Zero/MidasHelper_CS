                        <TextBlock TextWrapping="Wrap" Text="扫地杆高度h2" Margin="6,149,0,0" HorizontalAlignment="Left" Width="81" Height="18" VerticalAlignment="Top" />
                        <TextBox Height="23" HorizontalAlignment="Left" Margin="304,144,0,0" Name="text_h3" VerticalAlignment="Top" Width="65" TextChanged="text_h3_TextChanged" />
                        <TextBlock TextWrapping="Wrap" Text="下部可调托撑高度h3" Margin="182,147,0,0" HorizontalAlignment="Left" Width="118" Height="20" VerticalAlignment="Top" />
                        <TextBox Height="23" HorizontalAlignment="Left" Margin="96,178,0,0" Name="text_l1" VerticalAlignment="Top" Width="50" TextChanged="text_l1_TextChanged" />
                        <TextBlock TextWrapping="Wrap" Text="剪刀撑面间距l1" Margin="8,181,0,0" HorizontalAlignment="Left" Width="89" Height="20" VerticalAlignment="Top" />
                        <TextBox Height="23" HorizontalAlignment="Left" Margin="248,175,0,0" Name="text_l2" VerticalAlignment="Top" Width="52" TextChanged="text_l2_TextChanged" />
                        <TextBlock TextWrapping="Wrap" Text="剪刀撑线间距l2" Margin="162,178,0,0" HorizontalAlignment="Left" Width="89" Height="20" VerticalAlignment="Top" />
                        <TextBlock TextWrapping="Wrap" Text="注:单位(m)" Margin="312,178,0,0" HorizontalAlignment="Left" Width="59" Height="26" VerticalAlignment="Top" />
                    </Grid>
                </GroupBox>
                <GroupBox Header="阶段设置" Height="198" HorizontalAlignment="Left" Margin="18,272,0,0" Name="groupBox2" VerticalAlignment="Top" Width="219">
