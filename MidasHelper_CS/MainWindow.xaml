                <Button Content="承载力计算器" Height="23" HorizontalAlignment="Left" Margin="736,447,0,0" Name="btn_cacul" VerticalAlignment="Top" Width="86" />
                <Button Content="测试" Height="23" HorizontalAlignment="Left" Margin="296,450,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
                <CheckBox Content="屈曲分析" Height="16" HorizontalAlignment="Left" Margin="122,478,0,0" Name="check_buckle" VerticalAlignment="Top" Checked="check_buckle_Checked" />
                <CheckBox Content="非线性分析" Height="16" HorizontalAlignment="Left" Margin="208,478,0,0" Name="check_nolinear" VerticalAlignment="Top" Checked="check_nolinear_Checked" />
                <Button Content="缺陷参数" Height="23" HorizontalAlignment="Left" Margin="377,450,0,0" Name="btn_defect" VerticalAlignment="Top" Width="75" Click="btn_defect_Click" />
